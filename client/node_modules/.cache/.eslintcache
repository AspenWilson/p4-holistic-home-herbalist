perties = recipe.properties.map((property) => {\n        return (\n          <li key={property.id}>{property.name}</li>\n        );\n      });\n    \n      const herbsList = recipe.herbs.length > 0 ? (\n        recipe.herbs.map((herb) => {\n          return (\n            <li key={herb.id}>{herb.name}</li>\n          );\n        }) \n      ) : (\n        <h3>No herbs have been added to this recipe.</h3>\n      );\n\n      const comments = recipe.comments.length > 0 ? (\n        recipe.comments.map((comment) => {\n            return (\n                <>\n                <h3>{comment.comment}</h3>\n                <h3>Posted by: </h3>\n                </>\n            )\n        })\n      ) : <h3> No comments have been posted to this recipe. </h3>\n    \n      return (\n        <div>\n          <h1>{recipe.name}</h1>\n          <h2>{recipe.directions}</h2>\n          <h3>{recipe.entered_by}</h3>\n          <ul>{ingredientsList}</ul>\n          <ul>{recipeProperties}</ul>\n          <ul>{herbsList}</ul>\n          <ul>{comments}</ul>\n        </div>\n      );\n\n}\n\nexport default Recipe","/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/Property.js",[],"/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/context/LoadingContext.js",[],["137","138"],"/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/helpers.js",["139","140"],"/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/Search.js",["141"],"import React from 'react';\nimport { UserContext } from '../context/UserContext';\n\n\nfunction Search({ onSearch, searchTerm, setSearchTerm }) {\n\n  const handleInputChange = (e) => {\n    const userInput = e.target.value\n    setSearchTerm(userInput);\n    onSearch(userInput)\n  };\n\n\n  const handleClear = () => {\n    setSearchTerm('');\n  };\n\n  return (\n    <div className=\"search\">\n      <input\n        type=\"text\"\n        placeholder=\"Se