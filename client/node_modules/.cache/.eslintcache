nu.Item as={Link} to=\"/herbs\">\n                <Icon name='leaf' />\n                Herbs\n            </Menu.Item>\n            <Menu.Item as={Link} to=\"/recipes\">\n            <Icon name='utensils' />\n              Recipes\n        </Menu.Item>\n        <Menu.Item as={Link} to=\"/properties\">\n            <Icon name='unordered list'/>\n              Properties\n        </Menu.Item>\n        <Menu.Item as={Link} to=\"/\" onClick={handleLogout}>\n            <Icon name='sign out'/>\n              Logout\n        </Menu.Item> */}\n        </Menu>\n        : \n        <Menu icon='labeled' inverted fluid widths={2}>\n            {authRequiredOptions}\n        </Menu>\n    )\n    ) \n\n}\n\nexport default NavBar\n\n//     const NavH1 = styled.h1`\n// font-family: 'Splash', cursive;\n// `\n// const Nav = styled.div`\n//   display: flex;\n//   justify-content:space-between;\n  \n// `;\n\n// const Menu = styled.div`\n//   display: flex;\n//   align-items: center;\n//   a{\n//     text-decoration: none;\n//     color:white;\n//     font-family:Arial;\n//   }\n//   a:hover{\n//     color:pink\n//   }\n//   ul{\n//     list-style:none;\n//   }\n  \n// `;","/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/Herbs.js",["118","119","120","121"],"/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/HerbCard.js",["122","123"],"import React, {useState, useEffect, useContext} from 'react'\nimport { Card, Image, Label, Button } from 'semantic-ui-react'\nimport \"../index.css\"\nimport {Link} from 'react-router-dom'\nimport { propertyTags, url } from '../helpers'\nimport { UserContext } from '../context/UserContext';\n\n\nfunction HerbCard ({herb}) {\n    const { savedHerbs, user, fetchUpdatedData} = useContext(UserContext)\n    const [isSaved, setIsSaved] = useState(savedHerbs.some((savedHerb) => savedHerb.id === herb.id))\n    \n    useEffect(() => {\n        fetchUpdatedData()\n        console.log('useEffect from HerbCard')\n    },[])\n\n    const handleSave = () => {\n        fetch(`/api/users/${user.id}/saved-herbs`, {\n            method: 'POST',\n            headers: {\n                'Content-type': 'application/json',\n            },\n            body: JSON.stringify({herb_id: herb.id})\n        })\n        .then((resp) => {\n            if (resp.ok) {\n                setIsSaved(!isSaved)\n                fetchUpdatedData()\n                \n            }\n        })\n        .catch((error) => {\n            console.error('Error:', error)\n        })\n    }\n\n    const handleUnSave = () => {\n        fetch (`/api/users/${user.id}/saved-herbs/${herb.id}`, {\n            method:'DELETE'\n        })\n        .then((resp) => {\n            setIsSaved(!isSaved)\n            fetchUpdatedData()\n        })\n        .catch((error) => {\n            console.error('Error: ', error)\n        })\n    }\n\n\n    return (\n        <div key={herb.id}>\n            <Card raised>\n                <Card.Content>\n                    <Image\n                        src={herb.image_url}\n                        floated='left'\n                        size=\"small\"\n                        />\n                    <Card.Header>{herb.name}</Card.Header>\n                    <Card.Meta>{herb.latin_name}</Card.Meta>\n                    <Card.Description >\n                        <h3>Properties</h3>\n                        <Label.Group tag>\n                            {herb.properties.length > 0 ? (\n                            propertyTags(herb) \n                            ) : (null)}\n                        </Label.Group>\n                    </Card.Description>\n                </Card.Content>\n                <Card.Content extra>\n                    <Button basic color='green'>\n                        <Link to={`/herbs/${herb.id}`}>View Details</Link> \n                    </Button>\n                    {isSaved ? \n                        <Button basic color='orange' onClick={handleUnSave}>Saved</Button> :\n                        <Button basic color='yellow' onClick={handleSave}>Save</Button>\n                    }\n                </Card.Content>\n            </Card>\n        </div>\n    )\n}\n\n    export default HerbCard","/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/Recipes.js",[],"/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/Properties.js",["124"],"import React, {useContext} from 'react'\nimport PropertyCard from './PropertyCard'\nimport { UserContext } from '../context/UserContext';\nimport {Card} from 'semantic-ui-react'\n\n\nfunction Properties() {\n    const { properties, user } = useContext(UserContext)\n\n    const allProperties = properties.map((property) =>{\n        return (\n        <PropertyCard property={property} key={property.id}/>\n        )\n    })\n    return (\n        <Card.Group itemsPerRow={1}>\n            {allProperties}\n        </Card.Group>\n    )\n}\n\nexport default Properties","/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/Home.js",["125"],"import React from \"react\"\nimport { UserContext } from '../context/UserContext';\n\n\nfunction Home() {\n    return(\n        <div>\n            <header>Welcome to Holistic Home Herbalist</header>\n            <h1>space for what herbalism is</h1>\n            <h1>space for why it's important</h1>\n            <h1>space for what the site is and how to use it</h1>\n        </div>\n    )\n}\n\nexport default Home","/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/RecipeCard.js",["126"],"import React, {useContext, useState, useEffect } from \"react\";\nimport { Card, Label, Button } from 'semantic-ui-react'\nimport {Link} from 'react-router-dom'\nimport { propertyTags } from \"../helpers\";\nimport { UserContext } from '../context/UserContext';\n\n\nfunction RecipeCard({recipe}){\n    const { savedRecipes, user, fetchUpdatedData} = useContext(UserContext)\n    const [isSaved, setIsSaved] = useState(savedRecipes.some((savedRecipe) => savedRecipe.id === recipe.id))\n\n    useEffect(() => {\n        fetchUpdatedData()\n        console.log('useEffect from RecipeCard')\n    },[])\n\n    const handleSave = () => {\n        fetch(`/api/users/${user.id}/saved-recipes`, {\n            method: 'POST',\n            headers: {\n                'Content-type': 'application/json',\n            },\n            body: JSON.stringify({recipe_id: recipe.id})\n        })\n        .then((resp) => {\n            if (resp.ok) {\n                setIsSaved(!isSaved)\n                fetchUpdatedData()\n                \n            }\n        })\n        .catch((error) => {\n            console.error('Error:', error)\n        })\n    }\n\n    const handleUnSave = () => {\n        fetch (`/api/users/${user.id}/saved-recipes/${recipe.id}`, {\n            method:'DELETE'\n        })\n        .then((resp) => {\n            setIsSaved(!isSaved)\n            fetchUpdatedData()\n        })\n        .catch((error) => {\n            console.error('Error: ', error)\n        })\n    }\n\n\n    return (\n        <div key={recipe.id}>\n            <Card raised>\n                <Card.Content>\n                    <Card.Header>{recipe.name}</Card.Header>\n                    <Card.Description >\n                        <h3>Properties</h3>\n                        <Label.Group tag>\n                            {recipe.properties.length > 0 ? (\n                            propertyTags(recipe) \n                            ) : (null)}\n                        </Label.Group>\n                    </Card.Description>\n                </Card.Content>\n                <Card.Content extra>\n                        <Button basic color='green'>\n                        <Link to={`/recipes/${recipe.id}`}>View Details</Link> \n                    </Button>\n                    {isSaved ? \n                        <Button basic color='orange' onClick={handleUnSave}>Saved</Button> :\n                        <Button basic color='yellow' onClick={handleSave}>Save</Button>\n                    }\n                </Card.Content>\n            </Card>\n        </div>\n    )\n}\n\nexport default RecipeCard","/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/PropertyCard.js",["127","128","129"],"import React, { useContext, useState } from \"react\";\nimport { Card } from 'semantic-ui-react'\nimport { UserContext } from '../context/UserContext';\n\n\nfunction PropertyCard({property}){\n\n    return (\n        <Card raised key={property.id}>\n            <Card.Content as='h3'>{property.name}</Card.Content>\n            <Card.Content>{property.description}</Card.Content>\n        </Card>\n    )\n\n}\n\nexport default PropertyCard","/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/Herb.js",["130","131"],"import React, {useEffect, useState} from \"react\";\nimport { Image } from 'semantic-ui-react'\nimport { useParams } from 'react-router-dom';\nimport { url } from \"../helpers\";\nimport { UserContext } from '../context/UserContext';\n\n\nfunction Herb(){\n    const { id } = useParams();\n    const [herb, setHerb] = useState(null)\n\n    useEffect(() => {\n        fetch(`/api/herbs/${id}`)\n          .then((resp) => resp.json())\n          .then((data) => {\n            setHerb(data);\n          })\n          .catch((error) => {\n            console.error('Error fetching herb details:', error);\n          });\n      }, [id]);\n    \n      if (!herb) {\n        return <div>Loading...</div>;\n      }\n    \n      const dosagesList = herb.dosages.length > 0 ? (\n        herb.dosages.map((dosage) => {\n          return (\n            <li key={dosage.id}>\n              {dosage.dosage_form}: {dosage.dosage_description}\n            </li>\n          );\n        })\n      ) : (\n        <h3>No dosages listed for this herb.</h3>\n      );\n    \n      const herbProperties = herb.properties.map((property) => {\n        return (\n          <li key={property.id}>{property.name}</li>\n        );\n      });\n    \n      const recipesList = herb.recipes.length > 0 ? (\n        herb.recipes.map((recipe) => {\n          return (\n            <li key={recipe.id}>{recipe.name}</li>\n          );\n        }) \n      ) : (\n        <h3>No recipes with this herb have been created.</h3>\n      );\n    \n      return (\n        <div>\n          <Image src={herb.image_url} />\n          <h1>{herb.name}</h1>\n          <h2>{herb.latin_name}</h2>\n          <h3>{herb.description}</h3>\n          <h3>{herb.warnings}</h3>\n          <ul>{dosagesList}</ul>\n          <ul>{herbProperties}</ul>\n          <ul>{recipesList}</ul>\n        </div>\n      );\n    }\n\nexport default Herb","/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/Profile.js",["132","133"],"import React, {useState, useEffect, useContext} from 'react'\nimport { Tab, Grid, Header } from 'semantic-ui-react'\nimport \"../index.css\"\nimport Recipes from './Recipes';\nimport Herbs from './Herbs';\nimport { basicFetch } from '../helpers';\nimport { UserContext } from '../context/UserContext';\n\n\nfunction Profile() {\n    const { user, herbs, recipes, properties, fetchUpdatedData, savedHerbs, savedRecipes } = useContext(UserContext)\n    const [activeTab, setActiveTab] = useState('Saved Herbs')\n    const [enteredHerbs, setEnteredHerbs] = useState([])\n    const [enteredRecipes, setEnteredRecipes] = useState([])\n\n    const filteredHerbs = () => {\n        const herbsList = herbs.filter((herb) => herb.entered_by_id === user.id)\n        setEnteredHerbs(herbsList)\n    }\n\n    const filteredRecipes = () => {\n        const recipeList = recipes.filter((recipe) => recipe.entered_by_id === user.id)\n        setEnteredRecipes(recipeList)\n    }\n\n    useEffect(() => {\n        filteredHerbs()\n        filteredRecipes()\n        fetchUpdatedData()\n        console.log('useEffect from Profile')\n    },[])\n\n    const panes = [\n        { menuItem: 'Saved Herbs', render: () => (\n            <Tab.Pane>\n                {savedHerbs.length > 0 ? (\n                    <>\n                    <div>Saved Herbs</div>\n                    <Herbs profileHerbs={true}/>\n                    </>\n                ) : (<h3>No saved herbs.</h3>)}\n            </Tab.Pane> )},\n        { menuItem: 'Saved Recipes', render: () => (\n            <Tab.Pane>\n                {savedRecipes.length > 0 ? (\n                    <>\n                    <div>Saved Recipe</div>\n                    <Recipes profileRecipes={true}/>\n                    </>\n                ) : (<h3>No saved recipes.</h3>)}\n            </Tab.Pane> )},\n        { menuItem: 'Entered Herbs', render: () => (\n            <Tab.Pane>\n                {enteredHerbs.length > 0 ? (\n                        <Herbs herbs={enteredHerbs} savedHerbs={savedHerbs} user={user} properties={properties}/>\n                    ) : (\n                        <h3>No entered herbs.</h3>\n                    )}\n                    <div>Content for entered herbs</div>\n            </Tab.Pane> )},\n        { menuItem: 'Entered Recipes', render: () => (\n            <Tab.Pane>\n                {enteredRecipes.length > 0 ? (\n                    <Recipes recipes={enteredRecipes} user={user} properties={properties}/>\n                    ) : (\n                        <h3>No entered recipes.</h3>\n                    )}\n                    <div>Content for entered recipes</div>\n            </Tab.Pane> )},\n    ]\n\n    return (\n        <>\n        <div className='profile'>\n            <Grid>\n                <Grid.Column width={4}>\n                    <div className='avatar-circle'>\n                        {user.username[0]}\n                    </div>\n                </Grid.Column>\n                <Grid.Column width={12}>\n     