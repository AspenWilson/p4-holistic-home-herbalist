ch('/api/logout', {\n        method: 'DELETE'\n       })\n       .then(() => logout())\n    }\n\n\n    const authOptions =  \n    <>\n        <Menu.Item as={Link} to=\"/\">\n            <Icon name='home' />\n                Home\n        </Menu.Item>\n        <Menu.Item as={Link} to=\"/profile\">\n            <Icon name='user circle'/>\n                Profile\n        </Menu.Item>\n        <Menu.Item as={Link} to=\"/herbs\">\n            <Icon name='leaf' />\n                Herbs\n        </Menu.Item>\n        <Menu.Item as={Link} to=\"/recipes\">\n            <Icon name='utensils' />\n                Recipes\n        </Menu.Item>\n        <Menu.Item as={Link} to=\"/properties\">\n            <Icon name='unordered list'/>\n                Properties\n        </Menu.Item>\n        <Menu.Item as={Link} to=\"/\" onClick={handleLogout}>\n            <Icon name='sign out'/>\n                Logout\n        </Menu.Item> \n    </>\n\n    const authRequiredOptions =\n    <>\n        <Menu.Item as={Link} to=\"/login\">\n            <Icon name='sign in' />\n                Sign in/Sign up\n        </Menu.Item> \n    </>\n\n    return (\n        (loggedIn ? \n        <Menu icon='labeled' inverted fluid widths={6}>\n            {authOptions}\n            {/* <Menu.Item as={Link} to=\"/\">\n                <Icon name='home' />\n                Home\n            </Menu.Item>\n            <Menu.Item as={Link} to=\"/profile\">\n            <Icon name='user circle'/>\n              Profile\n        </Menu.Item>\n            <Menu.Item as={Link} to=\"/herbs\">\n                <Icon name='leaf' />\n                Herbs\n            </Menu.Item>\n            <Menu.Item as={Link} to=\"/recipes\">\n            <Icon name='utensils' />\n              Recipes\n        </Menu.Item>\n        <Menu.Item as={Link} to=\"/properties\">\n            <Icon name='unordered list'/>\n              Properties\n        </Menu.Item>\n        <Menu.Item as={Link} to=\"/\" onClick={handleLogout}>\n            <Icon name='sign out'/>\n              Logout\n        </Menu.Item> */}\n        </Menu>\n        : \n        <Menu icon='labeled' inverted fluid widths={2}>\n            {authRequiredOptions}\n        </Menu>\n    )\n    ) \n\n}\n\nexport default NavBar\n\n//     const NavH1 = styled.h1`\n// font-family: 'Splash', cursive;\n// `\n// const Nav = styled.div`\n//   display: flex;\n//   justify-content:space-between;\n  \n// `;\n\n// const Menu = styled.div`\n//   display: flex;\n//   align-items: center;\n//   a{\n//     text-decoration: none;\n//     color:white;\n//     font-family:Arial;\n//   }\n//   a:hover{\n//     color:pink\n//   }\n//   ul{\n//     list-style:none;\n//   }\n  \n// `;","/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/Herbs.js",["135","136","137","138","139"],"/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/HerbCard.js",["140","141"],"/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/Recipes.js",[],"/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/Properties.js",["142"],"import React, {useContext} from 'react'\nimport PropertyCard from './PropertyCard'\nimport { UserContext } from '../context/UserContext';\nimport {Card} from 'semantic-ui-react'\n\n\nfunction Properties() {\n    const { properties, user } = useContext(UserContext)\n\n    const allProperties = properties.map((property) =>{\n        return (\n        <PropertyCard property={property} key={property.id}/>\n        )\n    })\n    return (\n        <Card.Group itemsPerRow={1}>\n            {allProperties}\n        </Card.Group>\n    )\n}\n\nexport default Properties","/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/Home.js",["143"],"import React from \"react\"\nimport { UserContext } from '../context/UserContext';\n\n\nfunction Home() {\n    return(\n        <div>\n            <header>Welcome to Holistic Home Herbalist</header>\n            <h1>space for what herbalism is</h1>\n            <h1>space for why it's important</h1>\n            <h1>space for what the site is and how to use it</h1>\n        </div>\n    )\n}\n\nexport default Home","/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/RecipeCard.js",["144"],"import React, {useContext, useState, useEffect } from \"react\";\nimport { Card, Label, Button } from 'semantic-ui-react'\nimport {Link} from 'react-router-dom'\nimport { propertyTags } from \"../helpers\";\nimport { UserContext } from '../context/UserContext';\n\n\nfunction RecipeCard({recipe}){\n    const { savedRecipes, user, fetchUpdatedData} = useContext(UserContext)\n    const [isSaved, setIsSaved] = useState(savedRecipes.some((savedRecipe) => savedRecipe.id === recipe.id))\n\n    useEffect(() => {\n        fetchUpdatedData()\n        console.log('useEffect from RecipeCard')\n    },[])\n\n    const handleSave = () => {\n        fetch(`/api/users/${user.id}/saved-recipes`, {\n            method: 'POST',\n            headers: {\n                'Content-type': 'application/json',\n            },\n            body: JSON.stringify({recipe_id: recipe.id})\n        })\n        .then((resp) => {\n            if (resp.ok) {\n                setIsSaved(!isSaved)\n                fetchUpdatedData()\n                \n            }\n        })\n        .catch((error) => {\n            console.error('Error:', error)\n        })\n    }\n\n    const handleUnSave = () => {\n        fetch (`/api/users/${user.id}/saved-recipes/${recipe.id}`, {\n            method:'DELETE'\n        })\n        .then((resp) => {\n            setIsSaved(!isSaved)\n            fetchUpdatedData()\n        })\n        .catch((error) => {\n            console.error('Error: ', error)\n        })\n    }\n\n\n    return (\n        <div key={recipe.id}>\n            <Card raised>\n                <Card.Content>\n                    <Card.Header>{recipe.name}</Card.Header>\n                    <Card.Description >\n                        <h3>Properties</h3>\n                        <Label.Group tag>\n                            {recipe.properties.length > 0 ? (\n                            propertyTags(recipe) \n                            ) : (null)}\n                        </Label.Group>\n                    </Card.Description>\n                </Card.Content>\n                <Card.Content extra>\n                        <Button basic color='green'>\n                        <Link to={`/recipes/${recipe.id}`}>View Details</Link> \n                    </Button>\n                    {isSaved ? \n                        <Button basic color='orange' onClick={handleUnSave}>Saved</Button> :\n                        <Button basic color='yellow' onClick={handleSave}>Save</Button>\n                    }\n                </Card.Content>\n            </Card>\n        </div>\n    )\n}\n\nexport default RecipeCard","/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/PropertyCard.js",["145","146","147"],"import React, { useContext, useState } from \"react\";\nimport { Card } from 'semantic-ui-react'\nimport { UserContext } from '../context/UserContext';\n\n\nfunction PropertyCard({property}){\n\n    return (\n        <Card raised key={property.id}>\n            <Card.Content as='h3'>{property.name}</Card.Content>\n            <Card.Content>{property.description}</Card.Content>\n        </Card>\n    )\n\n}\n\nexport default PropertyCard","/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/Herb.js",[],"/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/Profile.js",["148","149"],"import React, {useState, useEffect, useContext} from 'react'\nimport { Tab, Grid, Header } from 'semantic-ui-react'\nimport \"../index.css\"\nimport Recipes from './Recipes';\nimport Herbs from './Herbs';\nimport { basicFetch } from '../helpers';\nimport { UserContext } from '../context/UserContext';\n\n\nfunction Profile() {\n    const { user, herbs, recipes, properties, fetchUpdatedData, savedHerbs, savedRecipes } = useContext(UserContext)\n    const [activeTab, setActiveTab] = useState('Saved Herbs')\n    const [enteredHerbs, setEnteredHerbs] = useState([])\n    const [enteredRecipes, setEnteredRecipes] = useState([])\n\n    const filteredHerbs = () => {\n        const herbsList = herbs.filter((herb) => herb.entered_by_id === user.id)\n        setEnteredHerbs(herbsList)\n    }\n\n    const filteredRecipes = () => {\n        const recipeList = recipes.filter((recipe) => recipe.entered_by_id === user.id)\n        setEnteredRecipes(recipeList)\n    }\n\n    useEffect(() => {\n        filteredHerbs()\n        filteredRecipes()\n        fetchUpdatedData()\n        console.log('useEffect from Profile')\n    },[])\n\n    const panes = [\n        { menuItem: 'Saved Herbs', render: () => (\n            <Tab.Pane>\n                {savedHerbs.length > 0 ? (\n                    <>\n                    <div>Saved Herbs</div>\n                    <Herbs profileHerbs={true}/>\n                    </>\n                ) : (<h3>No saved herbs.</h3>)}\n            </Tab.Pane> )},\n        { menuItem: 'Saved Recipes', render: () => (\n            <Tab.Pane>\n                {savedRecipes.length > 0 ? (\n                    <>\n                    <div>Saved Recipe</div>\n                    <Recipes profileRecipes={true}/>\n                    </>\n                ) : (<h3>No saved recipes.</h3>)}\n            </Tab.Pane> )},\n        { menuItem: 'Entered Herbs', render: () => (\n            <Tab.Pane>\n                {enteredHerbs.length > 0 ? (\n                        <Herbs herbs={enteredHerbs} savedHerbs={savedHerbs} user={user} properties={properties}/>\n                    ) : (\n                        <h3>No entered herbs.</h3>\n                    )}\n                    <div>Content for entered herbs</div>\n            </Tab.Pane> )},\n        { menuItem: 'Entered Recipes', render: () => (\n            <Tab.Pane>\n                {enteredRecipes.length > 0 ? (\n                    <Recipes recipes={enteredRecipes} user={user} properties={properties}/>\n                    ) : (\n                        <h3>No entered recipes.</h3>\n                    )}\n                    <div>Content for entered recipes</div>\n            </Tab.Pane> )},\n    ]\n\n    return (\n        <>\n        <div className='profile'>\n            <Grid>\n                <Grid.Column width={4}>\n                    <div className='avatar-circle'>\n                        {user.username[0]}\n                    </div>\n                </Grid.Column>\n                <Grid.Column width={12}>\n                    <Header as='h1'>{user.username}'s Profile</Header>\n                    <p>Joined: {user.account_created_on}</p>\n                </Grid.Column>\n            </Grid>\n\n            <Tab \n                menu={{ fluid: true, vertical: true, tabular: true }}\n                panes={panes}\n                activeIndex={panes.findIndex((pane) => pane.menuItem === activeTab)}\n                onTabChange={(_, data) => setActiveTab(data.panes[data.activeIndex].menuItem)}\n              />\n        </div>\n        </>\n    )\n\n}\n\nexport default Profile","/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/context/UserContext.js",["150","151","152"],"/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/Recipe.js",["153","154"],"import React, {useEffect, useState} from \"react\";\nimport { Image } from 'semantic-ui-react'\nimport { useParams } from 'react-router-dom';\nimport { UserContext } from '../context/UserContext';\n\n\n\nfunction Recipe(){\n    const { id } = useParams();\n    const [recipe, setRecipe] = useState(null)\n\n    useEffect(() => {\n        fetch(`/api/recipes/${id}`)\n          .then((resp) => resp.json())\n          .then((data) => {\n            setRecipe(data);\n          })\n          .catch((error) => {\n            console.error('Error fetching recipe details:', error);\n          });\n      }, [id]);\n    \n      if (!recipe) {\n        return <div>Loading...</div>;\n      }\n\n      const ingredientsList = recipe.ingredients.length > 0 ? (\n        recipe.ingredients.map((ingredient) => {\n          return (\n            <ul key={ingredient.id}>\n            <li>Herb Type: {ingredient.herb_type}</li>\n            <li>Amount: {ingredient.amount} {ingredient.amount_type}</li>\n            {/* <li>Herb: {ingredient.herb.name}</li> */}\n            </ul>\n          );\n        })\n      ) : (\n        <h3>No ingredients listed for this recipe.</h3>\n      );\n    \n      const recipeProperties = recipe.properties.map((property) => {\n        return (\n          <li key={property.id}>{property.name}</li>\n        );\n      });\n    \n      const herbsList = recipe.herbs.length > 0 ? (\n        recipe.herbs.map((herb) => {\n          return (\n            <li key={herb.id}>{herb.name}</li>\n          );\n        }) \n      ) : (\n        <h3>No herbs have been added to this recipe.</h3>\n      );\n\n      const comments = recipe.comments.length > 0 ? (\n        recipe.comments.map((comment) => {\n            return (\n                <>\n                <h3>{comment.comment}</h3>\n                <h3>Posted by: </h3>\n                </>\n            )\n        })\n      ) : <h3> No comments have been posted to this recipe. </h3>\n    \n      return (\n        <div>\n          <h1>{recipe.name}</h1>\n          <h2>{recipe.directions}</h2>\n          <h3>{recipe.entered_by}</h3>\n          <ul>{ingredientsList}</ul>\n          <ul>{recipeProperties}</ul>\n          <ul>{herbsList}</ul>\n          <ul>{comments}</ul>\n        </div>\n      );\n\n}\n\nexport default Recipe","/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/Pro