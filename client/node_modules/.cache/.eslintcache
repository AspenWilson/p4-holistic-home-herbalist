t);\n      },\n      (error) => {\n        console.log(error.text);\n      }\n    );\n  }\n  \n  return (\n    <Formik\n        initialValues={initialValues}\n        validationSchema={formSchema}\n        enableReinitialize={true}\n        onSubmit={sendEmail}\n    >\n        {(formik) =>(\n            <Form>\n                <h2>Confirm your email:</h2>\n                <Input fluid \n                    name='email' \n                    type='text' \n                    value={formik.values['email']} \n                    onChange={formik.handleChange} \n                />\n                {formik.touched['email'] && formik.errors['email'] && (\n                    <div style={{ color: \"red\" }}>{formik.errors['email']}</div>\n                )}\n                <Button fluid style={{backgroundColor: 'black', color:'white', font:'Arial' }} type='submit'>Send email!</Button>\n                <div>\n                    <div dangerouslySetInnerHTML={{ __html: message }} />\n                </div>\n            </Form>\n        )}\n    </Formik>\n  );\n}\n\nexport default HerbEmail;\n\n","/Users/aspen/Development/code/phase-4/p4-holistic-home-herbalist/client/src/components/emailjs/RecipeEmail.js",["409","410","411","412"],{"ruleId":"413","replacedBy":"414"},{"ruleId":"415","replacedBy":"416"},{"ruleId":"417","severity":1,"message":"418","line":21,"column":8,"nodeType":"419","endLine":21,"endColumn":24,"suggestions":"420"},{"ruleId":"421","severity":1,"message":"422","line":1,"column":42,"nodeType":"423","messageId":"424","endLine":1,"endColumn":51},{"ruleId":"417","severity":1,"message":"425","line":25,"column":8,"nodeType":"419","endLine":25,"endColumn":12,"suggestions":"426"},{"ruleId":"427","severity":1,"message":"428","line":60,"column":61,"nodeType":"429","messageId":"430","endLine":60,"endColumn":63},{"ruleId":"427","severity":1,"message":"428","line":63,"column":53,"nodeType":"429","messageId":"430","endLine":63,"endColumn":55},{"ruleId":"421","severity":1,"